{
    "type": "gpii.preferencesServer.config.ci.production",
    "options": {
        "distributeOptions": {
            "rawPreferencesDataSourceEnv": {
                "record": "@expand:kettle.resolvers.env(GPII_PREFERENCES_SERVER_URL)",
                "target": "{that preferencesServer rawPreferencesDataSource}.options.url",
                "priority": "after:rawPreferencesDataSource"
            },
            "distributePortEnv": {
                "record": "@expand:kettle.resolvers.env(GPII_PREFERENCES_SERVER_PORT)",
                "target": "{that kettle.server}.options.port",
                "priority": "after:distributePort"
            }
        }
    },
    "mergeConfigs": "./gpii.preferencesServer.config.production.json"
}
